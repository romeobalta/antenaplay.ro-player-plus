{
	"name": "AntenaPlay.ro Player Plus",
	"version": "0.0.1",
	"description": "Basic keybinds for AntenaPlay.ro player.",
  "manifest_version": 2,
  "icons": {
		"128": "128.png"
	},
  "content_scripts": [
    {
      "matches": [
        "https://*.antenaplay.ro/*",
        "http://*.antenaplay.ro/*",
        "https://antenaplay.ro/*",
        "http://antenaplay.ro/*"
      ],
      "run_at": "document_idle",
      "js": ["inject.js"]
    }
  ],
	"permissions": [
		"https://antenaplay.ro/",
		"http://antenaplay.ro/",
		"activeTab"
	]
}
